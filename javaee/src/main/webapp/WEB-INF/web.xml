<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>SeguridadFilter</filter-name>
  	<display-name>SeguridadFilter</display-name>
  	<description></description>
  	<filter-class>com.ipartek.formacion.filter.SeguridadFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>SeguridadFilter</filter-name>
  	<url-pattern>/privado/*</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.ejemplocapas.controladores.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>VideoServlet</servlet-name>
  	<display-name>VideoServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.ejemplocapas.controladores.VideoServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>VideoUnicoServlet</servlet-name>
  	<display-name>VideoUnicoServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.ejemplocapas.controladores.VideoUnicoServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GatosServlet</servlet-name>
  	<display-name>GatosServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.ejemplocapas.controladores.GatosServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.ejemplocapas.controladores.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ComprarServlet</servlet-name>
  	<display-name>ComprarServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.ejemplocapas.controladores.ComprarServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UsuarioController</servlet-name>
  	<display-name>UsuarioController</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.ejemplocapas.controladores.UsuarioController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>VideoServlet</servlet-name>
  	<url-pattern>/videos</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>VideoUnicoServlet</servlet-name>
  	<url-pattern>/video</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GatosServlet</servlet-name>
  	<url-pattern>/gatos</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ComprarServlet</servlet-name>
  	<url-pattern>/comprar</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UsuarioController</servlet-name>
  	<url-pattern>/usuarios</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  	<welcome-file>login</welcome-file>
  </welcome-file-list>
</web-app>

